<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>

    <div>
        <nav class="navbar navbar-fixed-top" style="background-color:black; border-bottom:6px solid black">
            <div class="container-fluid">
                <div>
                    <center><h3 style="color:white;font-family:Calibri">Software Incubator Mailer</h3></center>
                </div>
            </div>
        </nav>
        <br /><br /><br /><br />
        <div class="container body-content">
            @RenderBody()
            <hr />
        </div>
        @*<footer style="background-color:#999999; border-top:6px solid black; text-align: center;">
            &copy; @DateTime.Now.Year -<b>LOVE.PEACE>CODE</b>
        </footer>*@


        @RenderSection("scripts", required: false)

    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>